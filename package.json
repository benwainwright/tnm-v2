  "scripts": {
    "build": "gatsby build && yarn storybook:build",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path config/prettierignore --config config/prettier.config.js --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "deploy:frontend": "cdk deploy test-TnmV2AppStack",
    "deploy:frontend:prod": "cdk deploy prod-TnmV2AppStack",
    "deploy:backend": "cdk deploy test-TnmV2BackendStack --outputs-file src/backend-outputs.json",
    "deploy:backend:prod": "cdk deploy prod-TnmV2BackendStack --outputs-file src/backend-outputs.json",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:watch": "jest --config ./config/jest.config.ts --watch",
    "test": "jest --config ./config/jest.config.ts",
    "test:coverage": "jest --config ./config/jest.config.ts --coverage",
    "test:cypress": "yarn cypress run",
    "storybook": "start-storybook --port 6006 --static-dir ./src/common/fonts",
    "storybook:build": "build-storybook --static-dir ./src/common/fonts --output-dir ./public/storybook",
    "lint": "eslint .",
    "lint:styles": "stylelint  './src/**/*.css' './src/**/*.tsx' './src/**/*.ts'",
    "types": "tsc && tsc --project ./cypress/tsconfig.json",
    "build-storybook": "build-storybook"
  },
