{
  "exclude": [
    "cypress"
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "types": [
      "@types/testing-library__jest-dom",
      "node"
    ],
    "paths": {
      "@common/*": [
        "common/*"
      ]
    },
    "target": "es5",
    "lib": [
      "DOM",
      "ES2019"
    ],
    "noEmit": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
